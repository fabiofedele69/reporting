SELECT 'DIRECT' source, grantee, privilege, grantable
FROM dba_tab_privs
WHERE grantee = 'LC_SCMT'
  AND owner = 'SYS'
  AND table_name = 'DBMS_LOB'

UNION ALL

SELECT 'ROLE' source, p.grantee, p.privilege, p.grantable
FROM dba_tab_privs p
JOIN dba_role_privs r
  ON p.grantee = r.granted_role
WHERE r.grantee = 'LC_SCMT'
  AND p.owner = 'SYS'
  AND p.table_name = 'DBMS_LOB';



SELECT grantee, privilege, grantable
FROM dba_tab_privs
WHERE owner='SYS'
AND table_name='DBMS_LOB'
ORDER BY grantee;


extra checks

SELECT owner, object_name, object_type
FROM dba_objects
WHERE object_name = 'YOUR_OBJECT_NAME';


SELECT *
FROM dba_tab_privs
WHERE grantee='PUBLIC'
AND table_name='DBMS_LOB';


